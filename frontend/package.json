    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "tslint-config-ark120202": "^1.0.0",
    "tslint-config-prettier": "^1.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
