    "free-google-image-search": "^1.0.0",
    "jsonp": "^0.2.1",
    "pify": "^3.0.0",
    "vue": "^2.5.13",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@types/gapi": "^0.0.35",
    "@types/jsonp": "^0.2.0",
    "@types/pify": "^3.0.0",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "autoprefixer": "^7.2.4",
